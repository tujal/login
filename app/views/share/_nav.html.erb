<% if user_signed_in? %>
    <nav class="navbar bg-white">
      <div> <img src="https://cdn-icons-png.flaticon.com/128/733/733558.png", style="width:30px; height: 30px"><%= image_tag("insta_logo.png", size:"150x30")%></div>
     <%= render  "share/search" %>
      <div> 
            <%= link_to root_path do %>
            <img src="https://cdn-icons-png.flaticon.com/128/1946/1946488.png", style="width:30px; height: 30px">
            <% end %>
            <%= link_to new_user_post_path(current_user) do %>
                         <img src="https://cdn-icons-png.flaticon.com/128/11366/11366824.png", style="width:30px; height: 30px">
             <% end %>
     </div>
            <% if user_signed_in? %>
               
                <span><b><%= current_user.first_name %><%= current_user.last_name %></b>  <%= link_to 'Edit profile', edit_user_registration_path, class:"btn btn-primary btn-sm" %>  <%= link_to "Sign_out", destroy_user_session_path, data:{ turbo_method: :delete} , class:"btn btn-outline-danger btn-sm" %>
                </span>
            <% else %>
            <div>
                <%= link_to "Sign_in", new_user_session_path, class:"btn btn-success btn-sm" %>
                <%= link_to "Sign_up", new_user_registration_path , class:"btn btn-outline-primary btn-sm" %>
            </div>
            <% end %>
    </nav>
<% end %>